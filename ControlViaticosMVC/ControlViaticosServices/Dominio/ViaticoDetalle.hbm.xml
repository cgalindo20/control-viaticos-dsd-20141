<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ControlViaticosServices"
                   namespace="ControlViaticosServices.Dominio"
                   default-lazy="false">
  <class name="ViaticoDetalle" table="T_SOLICITUD_DETALLE">
    <composite-id name="PK">
      <key-property name="Viatico" column="Co_Solicitud" />
      <key-many-to-one name="TipoViatico" column="Co_TipoViatico" />
    </composite-id>    
    <property name="Ss_MontoSolicitado" column="Ss_MontoSolicitado" />    
  </class>
</hibernate-mapping>
